<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <appSettings>
        <!-- IMEの制御方法 on:IMEオンにする off:グローバル -->
        <add key="AlwaysIMEMode" value="on" />
        <!-- 既定でIMEオフにするアプリ名を指定。カンマ,で区切る。 -->
        <add key="ImeOffList" value="cmd,WindowsTerminal," />
        <!-- IMEオフにするタイトルの一部を正規表現で指定 -->
        <add key="ImeOffTitle" value="英語の文書.txt,\.py," />
        <!-- 無視するアプリ名 -->
        <add key="PassList" value="Jane2ch," />
        <!-- 更新間隔をミリ秒（ms）で指定。1000 = 1秒間隔 -->
        <add key="intervalTime" value="500" />
        <!-- 未入力でIMEオンにする時間を分（min）で指定 -->
        <add key="NoKeyInputTime" value="1.5" />
        <!-- 「少し無効」にしてから再開するまでの時間 -->
        <add key="SuspendFewTime" value="3" />
        <!-- 「しばらく無効」にしてから再開するまでの時間 -->
        <add key="SuspendTime" value="45" />
        <!-- アクティブに移動を監視するアプリ名 -->
        <add key="OnActivatedAppList" value="MyApp,MeApp" />
        <!-- 連携するアプリケーション -->
        <add key="OnActivatedAppPath" value="C:\Program Files\SkipUAC\SkipUAC.exe" />
        <!-- 引数 -->
        <add key="OnActivatedArgv" value="/ID foo" />
        <!-- 非アクティブに移動を監視するアプリ名 -->
        <add key="BackgroundAppList" value="chrome,msedge" />
        <!-- 連携するアプリケーション -->
        <add key="BackgroundAppPath" value="C:\Program Files\SkipUAC\SkipUAC.exe" />
        <!-- 引数 -->
        <add key="BackgroundArgv" value="/ID bar" />
        <!-- 非アクティブになってから呼び出すまでの遅延 -->
        <add key="DelayBackgroundTime" value="3" />
    </appSettings>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
</configuration>
