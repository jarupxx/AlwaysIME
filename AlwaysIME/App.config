<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <appSettings>
        <!-- IMEの制御方法を指定します。on:IMEオンにする、off:グローバル -->
        <add key="AlwaysIMEMode" value="on" />
        <!-- 既定でIMEオフにするアプリ名を指定します。カンマ,で区切ってください。 -->
        <add key="ImeOffList" value="cmd,WindowsTerminal," />
        <!-- IMEオフにするタイトルの一部を指定します。 -->
        <add key="ImeOffTitle" value="英語の文書.txt," />
        <!-- 更新間隔をミリ秒（ms）で指定します。1000 = 1秒間隔 -->
        <add key="intervalTime" value="500" />
    </appSettings>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
</configuration>
