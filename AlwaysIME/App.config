<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <appSettings>
        <!-- 更新間隔を指定します。1000 = 1秒間隔 -->
        <add key="intervalTime" value="1000" />
        <!-- 無視するアプリ名を指定します。 カンマ,で区切ってください。 -->
        <add key="AppList" value="AlwaysIME," />
    </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
</configuration>
