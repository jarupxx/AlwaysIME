<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <appSettings>
        <!-- 更新間隔をミリ秒(ms)で指定します。1000 = 1秒間隔 -->
        <add key="intervalTime" value="1000" />
        <!-- 既定でIMEオフにするアプリ名を指定します。 カンマ,で区切ってください。 -->
        <add key="ImeOffList" value="cmd,WindowsTerminal," />
        <!-- 無視するアプリ名を指定します。 カンマ,で区切ってください。 -->
        <add key="AppList" value="Jane2ch," />
        <!-- 未入力でIMEをオンにする時間を分(min)で指定します。 -->
        <add key="NoKeyInputTime" value="3" />
        <!-- Firewallでブロックするアプリ名を指定します。 カンマ,で区切ってください。 -->
        <add key="FWBlockList" value="MyApp,MeApp" />
        <!-- Firewallを許可するまでの時間を指定します。 -->
        <!-- intervalTime x FWAllowCount 遅延します。 -->
        <add key="FWAllowCount" value="300" />
        <!-- Firewallでブロックするときに使用するアプリを指定します。 -->
        <add key="appPathA" value="C:\Program Files\SkipUAC\SkipUAC.exe" />
        <!-- 引数を指定します。 -->
        <add key="argvA" value="/ID foo" />
        <!-- Firewallを許可するときに使用するアプリを指定します。 -->
        <add key="appPathB" value="C:\Program Files\SkipUAC\SkipUAC.exe" />
        <!-- 引数を指定します。 -->
        <add key="argvB" value="/ID bar" />
    </appSettings>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
</configuration>
